{
  "name": "mcp-db-census-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint migrations/ tests/",
    "migrate:create": "npx tsx node_modules/.bin/node-pg-migrate create --migration-file-language ts",
    "migrate:down": "npx tsx node_modules/.bin/node-pg-migrate down --verbose -f migrate.js",
    "migrate:up": "npx tsx node_modules/.bin/node-pg-migrate up --verbose -f migrate.js",
    "prod:build": "docker compose --profile prod up --build -d && npm run seed:prod",
    "prod:down": "docker compose --profile prod down",
    "schema:dump": "docker compose --profile dev exec census-mcp-db-dev pg_dump --schema-only --no-owner --no-privileges --username=mcp_user_dev mcp_db_dev > schema.sql",
    "seed": "npm run build && node dist/seeds/scripts/seed-database.js",
    "seed:prod": "node dist/seeds/scripts/seed-database.js",
    "test": "vitest --printConsoleTrace=true --silent=false",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "dotenv": "^17.1.0",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.11.3",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.23",
    "@types/pg": "^8.10.9",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "express": "^5.1.0",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.4"
  }
}
