{
  "name": "mcp-server-census-api",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js || true",
    "start": "node dist/http-server.js",
    "start:local": "node dist/index.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "npm run build",
    "test": "vitest --printConsoleTrace=true --silent=false",
    "test:coverage": "vitest --coverage",
    "watch": "tsc --watch",
    "heroku-postbuild": "npm run build && cd ../mcp-db && npm install && npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "fastmcp": "^3.19.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.23",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.1.4"
  }
}
